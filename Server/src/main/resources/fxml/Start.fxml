<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Label?>

<VBox xmlns:fx="http://javafx.com/fxml/1" styleClass="vbox" fx:controller="StartController" fx:id="root"
      prefHeight="600" prefWidth="800">

    <Label text="Specify port"/>
    <TextField fx:id="text_port" maxWidth="100"/>
    <Button fx:id="button_port" text="Start server" onAction="#serverCreate"/>

</VBox>

